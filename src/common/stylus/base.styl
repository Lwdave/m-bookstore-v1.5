body
  font-size 14px
  font-family "miui", "Helvetica Neue",Helvetica, STHeiTi, sans-serif
  color #555

a
  cursor pointer
  text-decoration none
  color #666
  &:visited
    color #666

.container
  position absolute
  top 45px
  right 0
  bottom 0
  left 0
  overflow auto

.main-card
  border-bottom 10px solid #f5f5f5
  background-color #fff
  &:last-child
    border-bottom none
